{
    "name": "roblox-ts-game",
    "globIgnorePaths": ["**/package.json", "**/tsconfig.json", "**/biome.json"],
    "tree": {
        "$className": "DataModel",
        "ServerScriptService": {
            "$className": "ServerScriptService",
            "server": {
                "$path": "out/server"
            }
        },
        "ReplicatedStorage": {
            "$className": "ReplicatedStorage",
            "rbxts_include": {
                "$path": "include",
                "node_modules": {
                    "$className": "Folder",
                    "@rbxts": {
                        "$path": "node_modules/@rbxts"
                    }
                }
            },
            "shared": {
                "$path": "out/shared"
            }
        },
        "StarterPlayer": {
            "$className": "StarterPlayer",
            "StarterPlayerScripts": {
                "$className": "StarterPlayerScripts",
                "client": {
                    "$path": "out/client"
                }
            },
            "node_modules": {
                "$className": "Folder",
                "@client": {
                    "$path": "node_modules/@client"
                }
            }
        },
        "Workspace": {
            "$className": "Workspace",
            "$properties": {
                "FilteringEnabled": true
            }
        },
        "HttpService": {
            "$className": "HttpService",
            "$properties": {
                "HttpEnabled": true
            }
        },
        "SoundService": {
            "$className": "SoundService",
            "$properties": {
                "RespectFilteringEnabled": true
            }
        }
    }
}